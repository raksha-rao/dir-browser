<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app >

	<!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Filesys.html</welcome-file>
  </welcome-file-list>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>NodeDisplayServiceImpl</servlet-name>
    <servlet-class>filesys.server.NodeDisplayServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>NodeDisplayServiceImpl</servlet-name>
    <url-pattern>/filesys/dir</url-pattern>
  </servlet-mapping>
  
  

</web-app>
